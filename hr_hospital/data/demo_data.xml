<odoo>
    <!-- Demo data for Doctors -->
    <record id="doctor_john" model="hr_hospital.doctor">
        <field name="name">Dr. John Doe</field>
        <field name="specialization">Cardiologist</field>
    </record>

    <record id="doctor_jane" model="hr_hospital.doctor">
        <field name="name">Dr. Jane Smith</field>
        <field name="specialization">Pediatrician</field>
    </record>

    <record id="doctor_william" model="hr_hospital.doctor">
        <field name="name">Dr. William Brown</field>
        <field name="specialization">General Practitioner</field>
    </record>

    <!-- Demo data for Patients -->
    <record id="patient_mike" model="hr_hospital.patient">
        <field name="name">Mike Johnson</field>
        <field name="age">32</field>
        <field name="gender">male</field>
        <field name="doctor_id" ref="doctor_john"/>
    </record>

    <record id="patient_emma" model="hr_hospital.patient">
        <field name="name">Emma Davis</field>
        <field name="age">28</field>
        <field name="gender">female</field>
        <field name="doctor_id" ref="doctor_jane"/>
    </record>

    <record id="patient_liam" model="hr_hospital.patient">
        <field name="name">Liam Wilson</field>
        <field name="age">45</field>
        <field name="gender">male</field>
        <field name="doctor_id" ref="doctor_william"/>
    </record>
</odoo>
